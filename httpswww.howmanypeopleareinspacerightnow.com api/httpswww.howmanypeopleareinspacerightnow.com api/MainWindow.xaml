<Window x:Class="httpswww.howmanypeopleareinspacerightnow.com_api.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WPF API Example" Height="300" Width="400">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0" Height="80" Width="80">
            <TextBlock x:Name="number" TextWrapping="Wrap" Margin="8" FontSize="50" />
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Margin="1,7,0,0">
            <TextBlock x:Name="message" TextWrapping="Wrap" Margin="10"  />
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Margin="60,80,0,0" Height="160" Width="80">
            <TextBlock x:Name="names" TextWrapping="Wrap" FontSize="10" Height="160"  />
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Margin="240,80,0,0" Height="160" Width="80">
            <TextBlock x:Name="crafts" TextWrapping="Wrap" FontSize="10" Height="160"  />
        </StackPanel>
    </Grid>
</Window>
